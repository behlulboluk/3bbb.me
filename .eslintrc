{
    "parser": "babel-eslint",
    "extends": ["airbnb", "prettier", "prettier/react"],
    "plugins": ["react", "prettier"],
    "rules": {
      "no-unused-vars": "off",
      "prettier/prettier": ["error"],
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [".js", "jsx"]
        }
      ],
      "max-len": ["error", 130]
    },
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "arrowFunctions": true
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    }
  }
  